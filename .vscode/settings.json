{
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        {
            "language": "vue",
            "autoFix": true
        },
        {
            "language": "html",
            "autoFix": true
        },
        {
            "language": "javascript",
            "autoFix": true
        }
    ],
    "files.associations": {
        "*.prettierrc": "jsonc"
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/.prettierrc"
            ],
            "url": "http://json.schemastore.org/prettierrc"
        }
    ]
}